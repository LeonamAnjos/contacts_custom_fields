<section>
    <% if user_signed_in?  %>
      <%= link_to "Log out", destroy_user_session_path, method: :delete %><br />
      <h1>Welcome, <%= current_user.name %>!</h1>
    <% else %>
      <%= link_to "Log in", new_user_session_path %><br />
      <h1>Welcome!</h1>
    <% end %>
</section>



<p>Rails version: <%= Rails.version %></p>




